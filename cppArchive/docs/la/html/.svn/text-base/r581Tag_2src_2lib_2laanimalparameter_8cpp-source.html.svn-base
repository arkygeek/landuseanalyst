<a name="l00211"></a><a class="code" href="classLaAnimalParameter.html#a36b4978a4e6ad8651cd4d9114c24a44">00211</a> <span class="keywordtype">bool</span> <a class="code" href="classLaAnimalParameter.html#a36b4978a4e6ad8651cd4d9114c24a44">LaAnimalParameter::fromXml</a>(QString theXml)
<a name="l00212"></a>00212 {
<a name="l00213"></a>00213   <span class="comment">//qDebug("Loading animal parameter from xml");</span>
<a name="l00214"></a>00214   QDomDocument myDocument(<span class="stringliteral">"mydocument"</span>);
<a name="l00215"></a>00215   myDocument.setContent(theXml);
<a name="l00216"></a>00216   QDomElement myTopElement = myDocument.firstChildElement(<span class="stringliteral">"animalParameter"</span>);
<a name="l00217"></a>00217   <span class="keywordflow">if</span> (myTopElement.isNull())
<a name="l00218"></a>00218   {
<a name="l00219"></a>00219     <span class="comment">//TODO - just make this a warning</span>
<a name="l00220"></a>00220    <span class="comment">//qDebug("top element could not be found!");</span>
<a name="l00221"></a>00221   }
<a name="l00222"></a>00222   <span class="comment">//qDebug("AnimalParameter::fromXml - guid found : " + myTopElement.attribute("guid").toLocal8Bit());</span>
<a name="l00223"></a>00223   <a class="code" href="classLaGuid.html#baf1a3fdc9aad8f76c1c0f8175fd5591">setGuid</a>(myTopElement.attribute(<span class="stringliteral">"guid"</span>));
<a name="l00224"></a>00224   <span class="comment">//qDebug("AnimalParameter::fromXml - guid set to : " + guid().toLocal8Bit());</span>
<a name="l00225"></a>00225   <a class="code" href="classLaAnimalParameter.html#c49846adb29b3ec7128479839d74416b">mName</a>=<a class="code" href="classLaUtils.html#43a40dc009242ab5ac430639784a4db5">LaUtils::xmlDecode</a>(myTopElement.firstChildElement(<span class="stringliteral">"name"</span>).text());
<a name="l00226"></a>00226   <a class="code" href="classLaAnimalParameter.html#d729fc96c002b027aeebaf1c0811067f">mDescription</a>=<a class="code" href="classLaUtils.html#43a40dc009242ab5ac430639784a4db5">LaUtils::xmlDecode</a>(myTopElement.firstChildElement(<span class="stringliteral">"description"</span>).text());
<a name="l00227"></a>00227   <a class="code" href="classLaAnimalParameter.html#c723b926622dbcf5c69d30c0549bbf22">mAnimalGuid</a>=<a class="code" href="classLaUtils.html#43a40dc009242ab5ac430639784a4db5">LaUtils::xmlDecode</a>(myTopElement.firstChildElement(<span class="stringliteral">"animal"</span>).text());
<a name="l00228"></a>00228   <a class="code" href="classLaAnimalParameter.html#90d46d091063ef9d7bf331d45cd25252">mPercentTameMeat</a>=QString(myTopElement.firstChildElement(<span class="stringliteral">"percentTameMeat"</span>).text()).toFloat();
<a name="l00229"></a>00229   <a class="code" href="classLaAnimalParameter.html#c96722219c437c83b0c8395a60938773">mUseCommonGrazingLand</a>=QString(myTopElement.firstChildElement(<span class="stringliteral">"useCommonGrazingLand"</span>).text()).toInt();
<a name="l00230"></a>00230   <a class="code" href="classLaAnimalParameter.html#3e5b3d3ac50fd6eb9abf25cb7226e61e">mUseSpecificGrazingLand</a>=QString(myTopElement.firstChildElement(<span class="stringliteral">"useSpecificGrazingLand"</span>).text()).toInt();
<a name="l00231"></a>00231   <a class="code" href="classLaAnimalParameter.html#8d4ed8ff3bc3349f8989dc73e51bbb0b">mTDNCommonGrazingLand</a>=QString(myTopElement.firstChildElement(<span class="stringliteral">"foodValueOfCommonGrazingLand"</span>).text()).toInt();
<a name="l00232"></a>00232   <a class="code" href="classLaAnimalParameter.html#02a817c7ecf83c26ad2b957c2c76aae9">mTDNSpecificGrazingLand</a>=QString(myTopElement.firstChildElement(<span class="stringliteral">"foodValueOfSpecificGrazingLand"</span>).text()).toInt();
<a name="l00234"></a>00234   <span class="comment">//mAreaUnits=LaUtils::xmlDecode(myTopElement.firstChildElement("areaUnits").text());</span>
<a name="l00235"></a>00235   QString myAreaUnits = QString(myTopElement.firstChildElement(<span class="stringliteral">"areaUnits"</span>).text());
<a name="l00236"></a>00236   <span class="keywordflow">if</span> (myAreaUnits == <span class="stringliteral">"Dunum"</span>)
<a name="l00237"></a>00237   {
<a name="l00238"></a>00238     <a class="code" href="classLaAnimalParameter.html#bebff4a2b38d6c4549ea5cee87611786">mAreaUnits</a>=<a class="code" href="r581Tag_2src_2lib_2la_8h.html#d5c8cac9c62ffdb8a2223362226dee4710d869fda8ef9b3852491ed49722a677">Dunum</a>;
<a name="l00239"></a>00239   }
<a name="l00240"></a>00240   <span class="keywordflow">else</span> <span class="keywordflow">if</span> (myAreaUnits == <span class="stringliteral">"Hectare"</span>)
<a name="l00241"></a>00241   {
<a name="l00242"></a>00242     <a class="code" href="classLaAnimalParameter.html#bebff4a2b38d6c4549ea5cee87611786">mAreaUnits</a>=<a class="code" href="r581Tag_2src_2lib_2la_8h.html#d5c8cac9c62ffdb8a2223362226dee476bf9786325e8633eb9e90667c85b339d">Hectare</a>;
<a name="l00243"></a>00243   }
<a name="l00244"></a>00244 
<a name="l00245"></a>00245   <a class="code" href="classLaAnimalParameter.html#debf262f3921ddd339e82062014e55e8">mFodderUse</a>=QString(myTopElement.firstChildElement(<span class="stringliteral">"fodderUse"</span>).text()).toInt();
<a name="l00246"></a>00246   <span class="comment">// populate the fodder map</span>
<a name="l00247"></a>00247   <a class="code" href="classLaAnimalParameter.html#fc65eeb60c6ed886b6816d1afd57becc">mFoodSourceMap</a>.clear();
<a name="l00248"></a>00248   QDomNodeList myFodderCropsList = myDocument.elementsByTagName(<span class="stringliteral">"fodderCrop"</span>);
<a name="l00249"></a>00249   <span class="keywordflow">for</span> (<span class="keywordtype">int</span> myCounter=0; myCounter &lt; myFodderCropsList.size(); myCounter++)
<a name="l00250"></a>00250   {
<a name="l00251"></a>00251     QDomNode myFoodSourceNode = myFodderCropsList.item(myCounter);
<a name="l00252"></a>00252     QDomElement myFoodSourceElement = myFoodSourceNode.toElement();
<a name="l00253"></a>00253 
<a name="l00254"></a>00254     <span class="comment">// load the data from the xml into local variables</span>
<a name="l00255"></a>00255     QString myGuid = myFoodSourceElement.firstChildElement(<span class="stringliteral">"fodderCropGuid"</span>).text();
<a name="l00256"></a>00256     <span class="keywordtype">int</span> myFodderStrawChaff = myFoodSourceElement.firstChildElement(<span class="stringliteral">"fodderStrawChaff"</span>).text().toInt();
<a name="l00257"></a>00257     <span class="keywordtype">int</span> myGrain = myFoodSourceElement.firstChildElement(<span class="stringliteral">"fodderGrain"</span>).text().toInt();
<a name="l00258"></a>00258     <a class="code" href="classLaFoodSource.html">LaFoodSource</a> myFoodSource;
<a name="l00259"></a>00259 
<a name="l00260"></a>00260     <span class="comment">// setup the data to insert into the map</span>
<a name="l00261"></a>00261     myFoodSource.<a class="code" href="classLaFoodSource.html#33d16e20d9849cb16563038859c61664">setFodder</a>(myFodderStrawChaff);
<a name="l00262"></a>00262     myFoodSource.<a class="code" href="classLaFoodSource.html#4a66e5da344064e50c6e6caea9fee92b">setGrain</a>(myGrain);
<a name="l00263"></a>00263     <span class="comment">// insert data into map</span>
<a name="l00264"></a>00264     <a class="code" href="classLaAnimalParameter.html#fc65eeb60c6ed886b6816d1afd57becc">mFoodSourceMap</a>.insert(myGuid,myFoodSource);
<a name="l00265"></a>00265   } <span class="comment">// end of for loop</span>
<a name="l00266"></a>00266  <span class="comment">//qDebug("Number of food sources restored from xml: " + QString::number(mFoodSourceMap.count()).toLocal8Bit());</span>
<a name="l00267"></a>00267 
<a name="l00268"></a>00268   QString myFallowUsage = QString(myTopElement.firstChildElement(<span class="stringliteral">"fallowUsage"</span>).text());
<a name="l00269"></a>00269   <span class="keywordflow">if</span> (myFallowUsage == <span class="stringliteral">"High"</span>)
<a name="l00270"></a>00270   {
<a name="l00271"></a>00271     <a class="code" href="classLaAnimalParameter.html#07fe9150cca3a3f4cfbd29c770a2b777">mFallowUsage</a>=<a class="code" href="r581Tag_2src_2lib_2la_8h.html#315655b1bb5848c063491adffde62b1524c57acd029e3f96fede49402ea01e6f">High</a>;
<a name="l00272"></a>00272   }
<a name="l00273"></a>00273   <span class="keywordflow">else</span> <span class="keywordflow">if</span> (myFallowUsage == <span class="stringliteral">"Medium"</span>)
<a name="l00274"></a>00274   {
<a name="l00275"></a>00275     <a class="code" href="classLaAnimalParameter.html#07fe9150cca3a3f4cfbd29c770a2b777">mFallowUsage</a>=<a class="code" href="r581Tag_2src_2lib_2la_8h.html#315655b1bb5848c063491adffde62b158cfb9311a439a51b14159ed0970f398b">Medium</a>;
<a name="l00276"></a>00276   }
<a name="l00277"></a>00277   <span class="keywordflow">else</span> <span class="keywordflow">if</span> (myFallowUsage == <span class="stringliteral">"Low"</span>)
<a name="l00278"></a>00278   {
<a name="l00279"></a>00279     <a class="code" href="classLaAnimalParameter.html#07fe9150cca3a3f4cfbd29c770a2b777">mFallowUsage</a>=<a class="code" href="r581Tag_2src_2lib_2la_8h.html#315655b1bb5848c063491adffde62b157a352a3dd2accc1dd65a4538c3754ee8">Low</a>;
<a name="l00280"></a>00280   }
<a name="l00281"></a>00281   <span class="keywordflow">else</span>
<a name="l00282"></a>00282   {
<a name="l00283"></a>00283     <a class="code" href="classLaAnimalParameter.html#07fe9150cca3a3f4cfbd29c770a2b777">mFallowUsage</a>=<a class="code" href="r581Tag_2src_2lib_2la_8h.html#315655b1bb5848c063491adffde62b15c9d3e887722f2bc482bcca9d41c512af">None</a>;
<a name="l00284"></a>00284   }
<a name="l00285"></a>00285   <a class="code" href="classLaAnimalParameter.html#05fc2437c1a388827244b15080f3cc1d">mRasterName</a>=<a class="code" href="classLaUtils.html#43a40dc009242ab5ac430639784a4db5">LaUtils::xmlDecode</a>(myTopElement.firstChildElement(<span class="stringliteral">"rasterName"</span>).text());
<a name="l00286"></a>00286   <span class="keywordflow">return</span> <span class="keyword">true</span>;
<a name="l00287"></a>00287 }
